.navbar-header
    button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
    a.navbar-brand(href='#') #{brand}
.navbar-collapse.collapse
    ul.nav.navbar-nav
        - each val, key in nav
            if (id == key)
                li.active
                    a(href='#{key}') #{val}
            else
                li
                    a(href='#{key}') #{val}

